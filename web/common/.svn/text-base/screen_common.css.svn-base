/* Field of Dreams || Screen CSS
   Design inspired by: http://www.freecsstemplates.org
--------------------------------------------------------------------*/

* {
  margin: 0;
  padding: 0;
}
body {
  font: 12px Arial, Helvetica, sans-serif;
  margin: 20px 0;
  text-align: justify;
  line-height: 18px;
  color: #7754a4;
  background: #fff url(../imgs/shared/bgHeadMain.gif) repeat-x;
}

/* Header Wrapper 
--------------------------------------------------------------------*/

#headWrapper {
  background: url(../imgs/shared/bgHeadBigGraphic.jpg) no-repeat;
  margin: 0 auto;
  padding: 0 0 10px 0;
  width: 900px;
  height: 180px;
}

/* Main Content Wrapper 
--------------------------------------------------------------------*/

#mainContWrapper {
  background: url(../imgs/shared/bgBodyMain.gif) repeat-y;
  width: 900px;
  margin: 0 auto;
}

/* Main Content & End of Page
--------------------------------------------------------------------*/

#mainContent {
  background: url(../imgs/shared/bgOrnamentL.jpg) no-repeat left top; 
  float: left;
  width: 530px;
  padding: 0 0 20px 100px;
}
#mainContent .contContent {
  padding: 12px 20px 0 20px;
  min-height: 300px;
}
#eopTxt {
  clear: both;
  width: 100%;
  font-size: 10px;
  padding-top: 6px;
}

/* rH Nav Menu Pane
--------------------------------------------------------------------*/

#verticalRhNav {
  background: url(../imgs/shared/bgOrnamentR.jpg) no-repeat right top;
  float: right;
  width: 160px;
  padding: 0 80px 20px 20px;
}
#verticalRhNav .navImg, #verticalRhNav .navImg:hover {
  margin: 20px 0 10px 0;
}
#verticalRhNav .navImg {
  border: 5px solid #cbcdfd;
}
#verticalRhNav .navImg:hover {
  border: 5px solid #ebbcff;
}
#verticalRhNav .quote {
  margin: 0;
  font-size: 10px;
  line-height: 14px;
}
#verticalRhNav .quote:before, .quoteLg:before {
  content: url(../imgs/shared/iconQuoteSmL.gif);
  margin: 0 2px 0 0;
}
#verticalRhNav .quote:after, .quoteLg:after {
  content: url(../imgs/shared/iconQuoteSmR.gif);
  margin: 0 0 0 2px;
}

/* Footer
--------------------------------------------------------------------*/

#footBreak {
  clear: both; 
  height: 1px;
}
#footer {
  background: url(../imgs/shared/bgFooter.gif) no-repeat;
  width: 900px;
  height: 40px;
  margin: 0 auto;
  font-size: 10px;
  color: #ae96ec;
}
#footer .left {
  float: left;
  padding: 12px 0 0 100px;
}
#footer .right {
  float: right;
  padding: 12px 60px 0 0;
}

/* Text & Image Classes
--------------------------------------------------------------------*/

img {
  border: none;
}
.singleImgL a, .singleImgL a:hover, .singleImgR a, .singleImgR a:hover {
  border: none;
}
.singleImgL {
  float: left;
  margin: -6px 10px 0 0;
}
.singleImgR {
  float: right;
  margin: -6px 0 0 10px;
}
.smImgL {
  float: left;
  margin: 3px 6px 0 0;
}
.smImgR {
  float: right;
  margin: 3px 0 0 6px;
}
.aRight {
  float: right;
  text-align: right;
  margin: 0;
}
.smTxt {
  font-size: 10px;
}
.italic {
  font-style: italic;
}
.brB {
  clear: both;
}
/* blockquote */
.bq {
  margin: 6px 0 0 18px;
}
.bold {
  font-weight: bold;
}
.centre {
  text-align: center;
}

/* Layout General
--------------------------------------------------------------------*/

.halfL, .halfR {
  width: 235px;
  font-size: 10px;
}
.halfL.large, .halfR.large {
  font-size: 12px;
}
.halfL {
  float: left;
}
.halfR {
  float: right;
}
.pHolderL {
  float: left;
  width: 70px;
  text-align: right;
  margin: 0 6px 0 0;
  font-weight: bold;
}
.thirdL, .thirdR {
  width: 150px;
}
.thirdL {
  float: left;
  margin: 4px 20px 0 0;
}
.thirdR {
  float: right;
  margin: 4px 0 0 0;
}
.shoutBoxL, .shoutBoxR {
  width: 120px;
  font: 13px bold 'Comic Sans MS', Arial, Helvetica, sans-serif;
  padding: 4px;
  text-align: center;
  border: 1px dotted #cbcdfd;
  color: #d2d2d2;
}
.shoutBoxL {
  float: left;
  margin: 0 12px 0 0;
}
.shoutBoxR {
  float: right;
  margin: 0 0 0 12px;
}

/* Main Content & Other Area Links
--------------------------------------------------------------------*/

a {
  text-decoration: none;
  color: #cbcdfd;
}
a:hover {
  color: #7754a4;
}
a.txtLink {
  color: #7754a4;
  padding: 0 0 1px 0;
  border-bottom: 1px dotted #ffe;
}
a.txtLink:hover {
  color: #cbcdfd;
  border-bottom: 1px dotted #7754a4;
}
ul li a:hover, #verticalRhNav ul li a:hover, #verticalRhNav a {
  border: none;
}
.linkNext:after, .linkNext:hover:after {
  margin-left: 6px;
}
.linkNext:after {
  content: url(../imgs/shared/iconNextOff.png);
}
.linkNext:hover:after {
  content: url(../imgs/shared/iconNextOn.png);
}
.linkBack:before, .linkBack:hover:before {
  margin-right: 6px;
}
.linkBack:before {
  content: url(../imgs/shared/iconBackOff.png);
}
.linkBack:hover:before {
  content: url(../imgs/shared/iconBackOn.png);
}
/* 
  ##### ---------| OPACITY HACK(ette) |--------- ##### 
*/
.thirdL a:visited img, .thirdL a img, .thirdR a:visited img, .thirdR a img {
  filter: alpha(opacity=60);
  filter: progid:DXImageTransform.Microsoft.Alpha(opacity=60);
  -moz-opacity: 0.60;
  opacity: 0.6;
}
.thirdL a:hover img, .thirdR a:hover img { 
  filter: alpha(opacity=100);
  filter: progid:DXImageTransform.Microsoft.Alpha(opacity=100);
  -moz-opacity: 1.00;
  opacity: 1;
}


/* Typography
--------------------------------------------------------------------*/

h1 {
  margin: 0 0 18px 0;
  font: normal 24px 'Times New Roman', Times, serif;
  font-style: italic;
  letter-spacing: 1px;
  color: #ebbcff;
}
h2 {
  font-size: 12px;
  margin: 12px 0 8px 0;
  text-transform: uppercase;
  letter-spacing: 1px;
  font-weight: bold;
  color: #ebbcff;
}
.halfL h2, .halfR h2 {
  margin: 0;
  font-size: 10px;
}
#verticalRhNav h2 {
  margin: 12px 0 -12px 0;
}
p {
  margin-bottom: 6px;
}
b {
  letter-spacing: 1px;
}
hr {
  margin: 8px 0;
  border: 1px dotted #7754a4;
  border-width: 1px 0 0 0;
}
hr.spacer {
  margin: 8px 0;
  border: 1px dotted #ae96ec;
  border-width: 1px 0 0 0;
}

/* Lists
--------------------------------------------------------------------*/

.contContent ul {
  list-style: none;
  margin: 0 0 6px 0;
}
.noLink ul li {
  padding-left: 10px;
  margin: 0;
  background: url(../imgs/shared/iconSmRhTriangle.jpg) no-repeat left center;
}
#verticalRhNav ul {
  list-style: none;
  margin: 20px 0 0 0;
}
ul li a, ul li a:hover, #verticalRhNav ul li a, #verticalRhNav ul li a:hover {
  padding-left: 26px;
  margin: 4px 0 0 0;
}
ul li a, #verticalRhNav ul li a {
  background: url(../imgs/shared/iconList.png) no-repeat left center;
}
ul li a:hover, #verticalRhNav ul li a:hover {
  background: url(../imgs/shared/iconList2.png) no-repeat left center;
}
/* Emulate nested <UL>'s */
li.padL {
  margin: 0 0 0 6px;
}
li.padL:before {
  content: '+ ';
}
dl {
  margin: 12px 0 6px 0;
}
dt, dd {
  margin: 0;
  padding: 6px 0;
  border-bottom: 1px solid #8970c1;
}
dt {
  float: left;
  clear: both;
  width: 120px;
  font-weight: bold;
}
ol {
  margin: 0 0 0 36px;
}

/* HTML - Forms
--------------------------------------------------------------------*/

select, button, label {
  cursor: pointer;
}
select, textarea, .txtInput, button {
  font-size: 12px;
}
fieldset {
  margin: 18px 0 12px 0;
  border: 2px solid #ad85c7;
  padding: 20px 0 6px 6px;
  text-align: justify;
  background: #ebbcff url(../imgs/shared/iconEmailSm.png) no-repeat 96% 5%;
}
label, label.req {
  float: left;
  font-weight: bold;
  width: 100px;
  text-align: right;
  margin: 0 6px 0 0;
  white-space: nowrap;
  color: #ad85c7;
}
label.req:before {
  content: '* ';
}
select, textarea, .txtInput {
  float: left;
  line-height: normal;
  background-color: #fde4ff;
  border: 1px solid #ad85c7;
  margin: 0 4px 18px 0;
  color: #666;
}
select {
  padding: 1px 2px 2px;
}
textarea {
  width: 60%;
  height: 48px;
}
button {
  background-color: #cbcdfd;
  border: 1px solid #ad85c7;
  padding: 1px 6px;
  margin: 6px 20px 12px 10px;
  color: #ad85c7;
}
button:hover {
  outline: 1px solid #ad85c7;
}
button:before {
  content: '>> ';
  font: 14px 'Trebuchet MS', Arial, Helvetica, sans-serif;
  letter-spacing: -1px;
}
